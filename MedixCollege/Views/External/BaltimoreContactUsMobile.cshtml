
@{
    ViewBag.Title = "BaltimoreContactUsMobile";
}

<h2>BaltimoreContactUsMobile</h2>

<form id="contactUsMobilForm" name="contactUsMobilForm" method="post" action=" http://www.northamericantradeschools.ca/external/BaltimoreContactUsMobilePost" target="dummyframe">
    <input type="hidden" name="ORGID" value="240" />
    <input type="hidden" name="MailListID" value="240001" />
    <input type="hidden" name="CampusID" value="24002" />
    <div class="">
        <label class="" for="formfirstname">First Name: <span class="">*</span></label>
        <input type="text" class="" name="FirstName" id="formfirstname" />
    </div>
    <div class="">
        <label class="" for="formlastname">Last Name: <span class="">*</span></label>
        <input type="text" class="" name="Lastname" id="formlastname" />
    </div>
    <div class="">
        <label class="" for="formemail">Email: <span class="">*</span></label>
        <input type="email" class="" name="Email" for="formemail" />
    </div>
    <div class="">
        <label class="" for="formtelephone">Phone: <span class="">*</span></label>
        <input type="text" class="" name="Telephone" for="formtelephone" />
    </div>
    <div class="">
        <label class="">Program: <span class="">*</span></label>
        <select class="" name="ProgramID" id="programsList">
            <option class="" value="0">Select</option>
            <option class="" value="39119">Building Construction Technology</option>
            <option class="" value="39120">Commercial Truck Driving</option>
            <option class="" value="39121">Diesel Technician</option>
            <option class="" value="39061">Electrical Technology</option>
            <option class="" value="30123">HRVAC Technology</option>
            <option class="" value="39125">Industrial Maintenance</option>
            <option class="" value="39129">Combination Welding</option>
            <option class="" value="40235">Motorcycle and Power Equipment Technology</option>
        </select>
    </div>
    <div class="">
        <input id="contactUsMobileSubmit" type="submit" value="Contact Us Today >" class="" />
    </div>
</form>


