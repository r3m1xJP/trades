@model List<TradesCollege.Models.LeadsDTO>
@{
    ViewBag.Title = "Leads";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
    <table class="table table-bordered">
        <tr>
            <th>Date</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Phone</th>
            <th>Email</th>
            <th>Location</th>
            <th>Program</th>
            <th>How did you hear about us?</th>
            <th>Comments</th>
        </tr>
        @foreach (var lead in Model)
        {
            <tr>
                <td>@lead.Date</td>
                <td>@lead.FirstName</td>
                <td>@lead.LastName</td>
                <td>@lead.Telephone</td>
                <td>@lead.Email</td>
                <td>@lead.Location</td>
                <td>@lead.Program</td>
        <td>@lead.HearAbout</td>
        <td>@lead.Comments</td>
    </tr>
        }
    </table>
</div>
